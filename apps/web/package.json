{
  "name": "@mach/web",
  "type": "module",
  "scripts": {
    "serve": "remix vite:dev",
    "pack": "remix vite:build"
  },
  "dependencies": {
    "@mach/web-details": "workspace:*",
    "@mach/web-home": "workspace:*",
    "@mach/web-search": "workspace:*",
    "@remix-run/cloudflare": "^2.9.1",
    "@remix-run/cloudflare-pages": "^2.9.1",
    "isbot": "^5"
  },
  "nx": {
    "tags": [
      "type:application"
    ],
    "targets": {
      "pack": {
        "outputs": [
          "{projectRoot}/build"
        ]
      }
    }
  },
  "devDependencies": {
    "vite-plugin-static-copy": "^1.0.6"
  }
}
